<tbody>
	<% for (let i = 0; i < list.length; i++) { %>
	<% const item = list[i]%>
	<tr>
		<td class="text-center">
			<%=i + 1 + ( ( pageNo - 1 ) * numOfRowPerPage )%>
		</td>
		<td>
			<a href="/employees/<%=item.employee.id%>?monthYear=<%=`${month}-${year}`%>">
				<%=item.employee.name%>
			</a>
		</td>
		<td>
			<%=item.numOfDayLate ?? 'N/A'%>
		</td>
		<td>
			<%=item.numOfDayEarly ?? 'N/A'%>
		</td>
		<td>
			<%=item.numofLE ?? 'N/A'%>
		</td>
	</tr>
	<% } %>
</tbody>